<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            xmlns:tools="http://schemas.android.com/tools"
            android:id="@+id/main_navigation"
            app:startDestination="@id/fragmentFirst">
	
	<fragment
			android:id="@+id/fragmentFirst"
			android:name="com.wisdom.navigationtest.FragmentFirst"
			android:label="fragment_first"
			tools:layout="@layout/fragment_first">
		<action
				
				android:id="@+id/action_fragmentFirst_to_fragmentSecond2"
				app:destination="@id/fragmentSecond"/>
	</fragment>
	<fragment
			android:id="@+id/fragmentSecond"
			android:name="com.wisdom.navigationtest.FragmentSecond"
			android:label="fragment_second"
			tools:layout="@layout/fragment_second"/>
</navigation>